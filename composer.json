{
    "name": "granam/web-versions",
    "description": "Reads available minor and patch versions of a web files repository",
    "autoload": {
        "psr-4": {
            "Granam\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Granam\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">= 7.1",
        "granam/git": "^1.1"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^9.4",
        "symfony/process": "^5.1"
    }
}
